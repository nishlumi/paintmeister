{
  	"name": "__MSG_appName__",
  	"description": "__MSG_appDescription__",
  	"version": "1.0.0.0",
  	"author" : "ISHII Eiju",
  	"manifest_version": 2,
  	"default_locale": "ja",
  	"key" : "",
  	"permissions": [
		"<all_urls>",
  		"app.runtime",
  		"app.window",
  		"storage",
		{"fileSystem": ["write", "retainEntries"]},
		"unlimitedStorage",
		"i18n"
  	],
  	"icons": {
  		"16": "images/paintm-16.png",
  		"32": "images/paintm-32.png",
  		"64": "images/paintm-64.png",
  		"128": "images/paintm-128.png"
  	},
  	"app": {
  		"background": {
  			"scripts": [
  				"js/main.js",
  				"js/chromereload.js"
  			]
  		}
  	}
  }